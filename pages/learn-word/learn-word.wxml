<!--pages/test/test.wxml-->
<view class="container3">
<van-row>

  <van-col span="8"> <view class="top-left" style="border-color: #46CA53;border-style:solid;border-width:2rpx 2rpx 2rpx 0rpx;">
    <text>需新学20词</text>
    <text>需复习20词</text>
  </view></van-col>


  <van-col span="16"><view class="top-right" style="border-color: #46CA53;border-style:solid;border-width:2rpx 0rpx 2rpx 0rpx;">
      <view class="top-left2">
      <image style="width: 40rpx; height: 40rpx;" mode="scaleToFill" src="../../images/hint.png"></image>
      <text>提示</text>
      </view>
      <view class="top-left2">
      <image style="width: 40rpx; height: 40rpx;" mode="scaleToFill" src="../../images/info.png"></image>
      <text>详情</text>
      </view>
      <view class="top-left2">
      <image style="width: 40rpx; height: 40rpx;" mode="scaleToFill" src="../../images/star.png"></image>
      <text>收藏</text>
      </view>
      <view class="top-left2">
      <image style="width: 40rpx; height: 40rpx;" mode="scaleToFill" src="../../images/prev.png"></image>
      <text>上一个</text>
      </view>
  </view></van-col>
</van-row>
<view class="container2">
  <view class="list-body">
      <view class="flex-wrp" style="flex-direction:row;">
      <view style="margin-top:0rpx">
      <text>请选择正确选项</text>
      </view>
      <view style="margin-top:40rpx">
      <image style="width: 600rpx; height: 400rpx; background-color: #eeeeee;margin-left:25rpx" mode="scaleToFill" src="https://img.yzcdn.cn/vant/cat.jpeg"></image>
     </view>
      <view wx:for="{{dictionary}}">
          <view class="item">
              <van-button round type="info" style="width:90%;margin-top:50rpx" color="#9EDDB2" block>添加计划</van-button>
          </view>
      </view>
      </view>
  </view>
</view>
</view>