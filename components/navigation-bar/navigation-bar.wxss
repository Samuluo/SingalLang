/* navigationBar.wxss */
.navigation-container {
  position: fixed;
  width: 100%;
  z-index: 99;
  top: 0;
  left: 0;
  background-image: linear-gradient(to right,  #83c6c2 ,#e1eded);

  border-radius: 0rpx 0rpx 30rpx 30rpx;
}
.navigation-bar {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navigation-buttons {
  display: flex;

  align-items: center;
  margin-left: 20px;
  box-sizing: border-box;
  border-radius: 50%;
  background-color: transparent;
}
.nav-img{
  height: 50rpx;
  width: 50rpx;
}
.navigation-title {
  position: absolute;
  left: 104px;
  right: 104px;
  text-align: center;
  font-size: 16px;
  font-weight: bold;
  color: #ffffff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
